<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <preference for="\Boundsoff\BrandNews\Api\FeedbackServiceInterface"
                type="\Boundsoff\BrandNews\Model\FeedbackService"/>
    <type name="\Boundsoff\BrandNews\Observer\CheckFeedbackObserver">
        <arguments>
            <argument name="dataObject" xsi:type="object">BoundsoffBrandNewsFeedbackData</argument>
        </arguments>
    </type>
    <virtualType name="BoundsoffBrandNewsFeedbackData" type="\Magento\Framework\DataObject">
        <arguments>
            <argument name="data" xsi:type="array">
                <item name="sandbox" xsi:type="array">
                    <item name="title" xsi:type="string" translatable="true">Sandbox</item>
                    <item name="description" xsi:type="string" translatable="true">
                        Hello World!
                    </item>
                    <item name="url" xsi:type="string">https://google.com/</item>
                </item>
            </argument>
        </arguments>
    </virtualType>
</config>
