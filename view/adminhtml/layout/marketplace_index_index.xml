<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Boundsoff_BrandNews::styles/marketplace.css"/>
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Boundsoff\BrandNews\Block\Adminhtml\Marketplace" name="boundsoff_index"
                   template="Boundsoff_BrandNews::marketplace/index.phtml">
                <arguments>
                    <argument name="feedback_service" xsi:type="object">
                        \Boundsoff\BrandNews\Api\FeedbackServiceInterface
                    </argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
